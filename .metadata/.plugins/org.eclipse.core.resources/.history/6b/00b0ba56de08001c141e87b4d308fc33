package com.lti;
import java.util.*;

class Student
{
	String s = new String("dheeraj,gaddam,deeksha,fruity,gopal");
}
public class Demo {
	public static void main(String a[])
	{
		Student stu = new Student();
		//System.out.println("name reversed  "+stu.s.reverse());
		StringTokenizer st = new StringTokenizer(stu.s,",");  
	    while(st.hasMoreTokens())
	    {
	    	System.out.print(st.nextToken()+" ");
	    }
	    String s = new String();
	    String d = new String("dheeraj");
		char[] c = d.toCharArray();
              for (char k : c)
              {
            	 s = s+k;
            		System.out.print(k+" ");
              }
	System.out.println(s);
	}
	

}
